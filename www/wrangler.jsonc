{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "seodatatools",
    "main": "./dist/_worker.js/index.js",
    "compatibility_date": "2025-08-05",
    "compatibility_flags": [
        "nodejs_compat",
        "global_fetch_strictly_public"
    ],
    "assets": {
        "binding": "ASSETS",
        "directory": "./dist"
    },
    "observability": {
        "enabled": true
    },
    "version_metadata": {
        "binding": "CF_VERSION_METADATA"
    }
}